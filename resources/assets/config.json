{
  "entry": {
    "main": [
      "./scripts/main.js",
      "./styles/main.scss"
    ],
    "admin": [
      "./scripts/admin.js",
      "./styles/admin.scss"
    ],
    "icons": [
      "./styles/icons.scss",
      "./scripts/loadcss.js"
    ],
    "customizer": [
      "./scripts/customizer.js"
    ],
    "vendor": [
      "./scripts/vendor.js"
    ]
  },
  "publicPath": "/app/themes/<example-project>",
  "devUrl": "http://<example-project>.dev",
  "proxyUrl": "http://localhost:3000",
  "cacheBusting": "[name]_[hash:8]",
  "watch": [
    "app/**/*.php",
    "config/**/*.php",
    "resources/controllers/**/*.php",
    "resources/views/**/*.twig"
  ],
  "rsync": {
    "hostname": "<example-project>.dev",
    "username": "vagrant",
    "destination": "/var/www/wordpress/web/app/themes/<example-project>",
    "watch": [
      "resources/assets/scripts/**/*.js",
      "app/**/*.php",
      "config/**/*.php",
      "resources/controllers/**/*.php",
      "resources/views/**/*.twig"
    ]
  }
}
